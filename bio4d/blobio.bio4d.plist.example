<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
 <key>Label</key>
 <string>blobio.bio4d</string>

 <!--
 	All environment vars defined here.
	All others vars defined in $BLOBIO_ROOT/etc/profile,
	sourced in by $BLOBIO_ROOT/sbin/launchd-bio4d.
 -->
 <key>EnvironmentVariables</key>
 <dict>
  
  <key>PATH</key>
  <!--  need zap-proc in /usr/local/jmscott -->
  <string>/usr/local/jmscott/bin:/usr/bin:/bin</string>

  <key>BLOBIO_ROOT</key>
  <string>/usr/local/blobio</string>

  <key>BLOBIO_BIO4D_PORT</key>
  <string>1797</string>

  <key>BLOBIO_BIO4D_RRD_DURATION</key>
  <string>60</string>

  <key>BLOBIO_BIO4D_NET_TIMEOUT</key>
  <string>20</string>

 </dict>

 <key>KeepAlive</key>
 <true/>

 <key>ProcessType</key>
 <string>Background</string>

 <!--  bio4d shutdowns down the kids -->
 <key>AbandonProcessGroup</key>
 <true/>

 <key>ProgramArguments</key>
 <array>
  <string>/usr/local/jmscott/sbin/launchd-log</string>
  <string>/usr/local/blobio/sbin/launchd-bio4d</string>
 </array>

 <key>RunAtLoad</key>
 <true/>

 <key>UserName</key>
 <string>blobio</string>

 <key>GroupName</key>
 <string>staff</string>

 <key>WorkingDirectory</key>
 <string>/usr/local/blobio</string>

 <key>StandardErrorPath</key>
 <string>/Users/blobio/log/launchd-bio4d.log</string>

 <key>StandardOutPath</key>
 <string>/Users/blobio/log/launchd-bio4d.log</string>

</dict>
</plist>
